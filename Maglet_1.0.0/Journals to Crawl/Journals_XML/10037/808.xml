publisher = {Kharazmi University},
title_fa = {ارزیابی حساسیت و پهنه بندی خطر زمین لغزش در قطعه 5 راه آهن اردبیل – میانه با استفاده از روش‌های تحلیل سلسله مراتبی فازی، مثلت سلسله مراتبی فولر و منطق فازی},
abstract_fa ={به نقشه درآوردن حساسیت زمین&#173;لغزش ابزاری اساسی برای مدیریت بلایا و فعالیت&#173;های توسعه برنامه&#173;ریزی در مناطق کوهستانی می&#173;باشد. نقشه&#173;های حساسیت ایجاد شده در این پژوهش بیانگر یک گام روبه جلو در زمینه مدیریت خطر زمین لغزش در محدوده قطعه 5 راه&#173;آهن اردبیل ـ میانه می&#173;باشد. اهداف اصلی این تحقیق، اولاً تولید نقشه&#173;های حساسیت زمین&#173;لغزش برای محدوده قطعه 5 راه&#173;آهن اردبیل ـ میانه و تأثیر آن بر شریان&#173;های حیاتی و ثانیاً تجزیه و تحلیل مهم&#8204;ترین عوامل تأثیر گذار در افزایش خطر زمین&#8204;لغزش در محدوده مورد مطالعه می&#173;باشند، بطوریکه استراتژی&#173;های مناسب کاهش ریسک زمین&#173;لغزش بتواند توسعه داده شود. در این پژوهش از سه روش منطق فازی، مثلث سلسله مراتبی فولر و تحلیل سلسله مراتبی فازی (F-AHP) استفاده شده است. روش&#173;های فوق&#173;الذکر از روش&#173;های تحلیل تصمیم&#173;گیری چند معیاره مبتنی بر سامانه اطلاعات جغرافیایی به منظور ارزیابی علمی نواحی مستعد زمین&#173;لغزش در محدوده قطعه 5 راه&#173;آهن اردبیل ـ میانه می&#173;باشند و برای نیل به این هدف از نرم&#173;افزارهای ArcGIS و&#160; IDIRISI استفاده شده است. همچنین روش&#173;های مثلث سلسله مراتبی فولر و سلسله مراتبی فازی (F-AHP) از طریق کد/ فرمول نویسی در محیط نرم&#173;افزار Excel اجرا شده است.&#160; 8 لایه اطلاعاتی برای انجام تحلیل&#173;ها درنظر گرفته شده است که عبارتند از: رده مقاومتی زمین، زاویه شیب، جهت شیب، رطوبت، پوشش گیاهی، فاصله از گسل، فاصله از رودخانه و نهایتاً فاصله از مسیرهای ارتباطی (جاده و راه&#173;آهن). سپس، برای تعیین میزان استعداد محدوده مورد مطالعه نسبت به رخداد زمین&#173;لغزش براساس روش منطق فازی، مثلث سلسله مراتبی فولر و سلسله مراتبی فازی (F-AHP) اقدام به پهنه&#173;بندی شده است. نهایتاً، عملکرد روش&#173;های مختلف در تولید نقشه&#173;های حساسیت زمین&#173;لغزش با استفاده از شاخص&#173;های صحت سنجی &#34;ضریب تراکم&#34; و &#34;جمع مطلوبیت&#34; با یکدیگر مقایسه شد تا روش مطلوب و کاربردی برای محدوده قطعه 5 راه&#173;آهن اردبیل ـ میانه به منظور مدیریت خطر زمین لغزش تعیین شود. با تحلیل نقشه&#173;های پهنه&#173;بندی حاصله و با توجه به با توجه به مقادیر جمع مطلوبیت حاصل از عملگر اجتماع فازی (0.329915245)، تحلیل سلسله مراتبی فازی (1.079120331) و مثلث سلسله مراتبی فولر (0.951658196) برای نقشه&#173;های&#173; پهنه&#173;بندی خطر زمین&#173;لغزش&#173;های قطعه 5 راه&#173;آهن میانه-اردبیل مشاهده می&#173;شود که جمع مطلوبیت حاصل از تحلیل سلسله مراتبی فازی، بیشتر از مقداری است که برای نقشه&#173;های&#173; پهنه&#173;بندی خطر حاصل از منطق فازی و مثلث سلسله مراتبی فولر محاسبه شده است},
keywords_fa = {زمین لغزش, منطق فازی, تحلیل سلسله مراتبی فازی, مثلث سلسله مراتبی فولر,  پهنه بندی خطر , سامانه اطلاعات جغرافیایی(ArcGIS)},
url = {http://jeg.khu.ac.ir/article-1-2814-en.html},  
eprint = {http://jeg.khu.ac.ir/article-1-2814-en.docx},  
journal = {Journal of Engineering Geology},  
issn = {2228-6837}, 
eissn = {}, 
year = {2020}  
}

@article{ 
author = {},  
title = {}, 
