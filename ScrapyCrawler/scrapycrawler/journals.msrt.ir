<script>
    $(function () {

        var interval = setInterval(updateDiv, 100);
        function updateDiv() {
            var result = $('[name = "Group"]').val();
            console.log(result);
            if (result != "") {
                clearInterval(interval);
                $('[name = "Group"]').change();
            }
        }

        $('[name="Group"]').on('change', function () {
            $('.fakelist').hide();
            if ($(this).val() != "") {
                $('[data-cdd="SubGroupId"]').parent().removeClass('hide')
            }
            else {
                $('[data-cdd="SubGroupId"]').parent().addClass('hide')
                $('.fakelist').show();
            }
        })

        var intervalUniversity = setInterval(updateDivUniversity, 500);
        function updateDivUniversity() {
            var resultt = $('[name = "Applicant"]').val();
            if (resultt != "") {
                clearInterval(intervalUniversity);
                $('[name = "Applicant"]').change();
            }
        }

        $('[name = "Applicant"]').on("change", function () {
            $('.fakelistApplicant').hide();
            var result = $('[name = "Applicant"]').val();

            if (parseInt(result) == 22) {
                $('#Association').removeClass("hide");
                $('#University').addClass("hide");
            }
            else if (parseInt(result) == 44 || parseInt(result) == 99) {
                $('#Association').addClass("hide");
                $('#University').addClass("hide");

                $('.fakelistApplicant').show();
            }
            else {
                $('#Association').addClass("hide");
                $('#University').removeClass("hide");
            }
        }).trigger('change')
    })
</script>

